<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".ChooseLevelActivity"
    android:background="@drawable/bgmain">

    <com.example.oneline.GraphicsChooseLevel
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.example.oneline.GraphicsChooseLevel>
    <TextView
        android:id="@+id/txtChoose"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Choose Level"
        android:textSize="40dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:gravity="center_horizontal"
        android:layout_marginTop="35dp"></TextView>
    <TableLayout
        android:layout_below="@+id/txtChoose"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TableRow>
            <Button
                android:id="@+id/level01"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="1"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level02"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="2"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level03"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="3"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level04"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="4"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
        </TableRow>
        <TableRow>
            <Button
                android:id="@+id/level05"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="5"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level06"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="6"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level07"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="7"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level08"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="8"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
        </TableRow>
        <TableRow>
            <Button
                android:id="@+id/level09"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="9"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level10"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="10"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level11"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="11"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
            <Button
                android:id="@+id/level12"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="25dp"
                android:background="@drawable/custom_buttonlevel"
                android:text="12"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="@color/black"></Button>
        </TableRow>


    </TableLayout>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/ic_launcher_iconback"
        android:layout_alignParentBottom="true"
        android:id="@+id/backstart"></ImageButton>

</RelativeLayout>